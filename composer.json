{
    "name": "MacroGear",
    "description": "MacroGear web application builder",
    "keywords": [
        "cms",
        "web",
        "commerce",
        "shop",
        "macrogear"
    ],
    "minimum-stability": "stable",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Maxim Papezhuk",
            "email": "invisliver@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "twig/twig": ">=1.5",
        "symfony/yaml": ">=3.0",
        "symfony/http-foundation": ">=3.0"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "gear/core",
            "Plugin\\": "gear/plugin",
            "App\\Model\\": "web/model",
            "App\\Module\\": "web/module",
            "App\\Tool\\": "web/tool"
        }
    },
    "config": {
        "vendor-dir": "gear/vendor"
    }
}